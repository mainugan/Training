{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adlstrainingdatafactoryv2"
		},
		"BlobMetadataSqlConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobMetadataSqlConnection'"
		},
		"DLSTargetProviderFunctionConnection_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'DLSTargetProviderFunctionConnection'"
		},
		"OrganizedDLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'OrganizedDLSConnection'"
		},
		"OrdersBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OrdersBlobStorage'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adlstrainingvault.vault.azure.net/"
		},
		"DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://dlstargetprocessor.azurewebsites.net/"
		},
		"OrganizedDLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadftrainingaccount.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adlstrainingdatafactoryv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DLSTargetProviderFunctionConnection_functionKey": {
						"value": "[parameters('DLSTargetProviderFunctionConnection_functionKey')]"
					},
					"OrganizedDLSConnection_accountKey": {
						"value": "[parameters('OrganizedDLSConnection_accountKey')]"
					},
					"OrdersBlobStorage_connectionString": {
						"value": "[parameters('OrdersBlobStorage_connectionString')]"
					},
					"BlobMetadataSqlConnection_connectionString": {
						"value": "[parameters('BlobMetadataSqlConnection_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('DLSTargetProviderFunctionConnection_properties_typeProperties_functionAppUrl')]"
					},
					"OrganizedDLSConnection_properties_typeProperties_url": {
						"value": "[parameters('OrganizedDLSConnection_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}